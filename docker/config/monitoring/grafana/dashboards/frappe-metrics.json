{
  "dashboard": {
    "title": "Frappe Press - Self-Hosted Platform Metrics",
    "uid": "frappe-press-selfhosted",
    "version": 1,
    "schemaVersion": 38,
    "timezone": "browser",
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "Container CPU Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{container_label_com_press_selfhosted_service=~\".+\"}[5m]) * 100",
            "legendFormat": "{{container_label_com_press_selfhosted_service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "CPU %"
          }
        ]
      },
      {
        "id": 2,
        "title": "Container Memory Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "container_memory_usage_bytes{container_label_com_press_selfhosted_service=~\".+\"} / 1024 / 1024",
            "legendFormat": "{{container_label_com_press_selfhosted_service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "mbytes",
            "label": "Memory (MB)"
          }
        ]
      },
      {
        "id": 3,
        "title": "Host CPU Usage",
        "type": "gauge",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Host Memory Usage",
        "type": "gauge",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Host Disk Usage",
        "type": "gauge",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\",fstype!=\"rootfs\"} / node_filesystem_size_bytes{mountpoint=\"/\",fstype!=\"rootfs\"}) * 100)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Container Network Rx",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        },
        "targets": [
          {
            "expr": "rate(container_network_receive_bytes_total{container_label_com_press_selfhosted_service=~\".+\"}[5m]) / 1024",
            "legendFormat": "{{container_label_com_press_selfhosted_service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "KBs",
            "label": "Receive Rate"
          }
        ]
      },
      {
        "id": 7,
        "title": "Container Network Tx",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        },
        "targets": [
          {
            "expr": "rate(container_network_transmit_bytes_total{container_label_com_press_selfhosted_service=~\".+\"}[5m]) / 1024",
            "legendFormat": "{{container_label_com_press_selfhosted_service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "KBs",
            "label": "Transmit Rate"
          }
        ]
      },
      {
        "id": 8,
        "title": "Container Count",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "targets": [
          {
            "expr": "count(container_last_seen{container_label_com_press_selfhosted_service=~\".+\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 9,
        "title": "System Load Average",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        },
        "targets": [
          {
            "expr": "node_load1",
            "legendFormat": "1m load",
            "refId": "A"
          },
          {
            "expr": "node_load5",
            "legendFormat": "5m load",
            "refId": "B"
          },
          {
            "expr": "node_load15",
            "legendFormat": "15m load",
            "refId": "C"
          }
        ]
      },
      {
        "id": 10,
        "title": "Disk I/O",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        },
        "targets": [
          {
            "expr": "rate(node_disk_read_bytes_total[5m]) / 1024 / 1024",
            "legendFormat": "Read {{device}}",
            "refId": "A"
          },
          {
            "expr": "rate(node_disk_written_bytes_total[5m]) / 1024 / 1024",
            "legendFormat": "Write {{device}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "MBs",
            "label": "I/O Rate"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  },
  "overwrite": true
}
